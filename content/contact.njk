---js
const eleventyNavigation = {
	key: "contact",
	order: 5
};
---

<h1>contact me</h1>

<body>
  <div class="wrapper">
    <form class="form form-entry" action="https://formspree.io/f/mlderoak" method="POST">
      <label for="name" class="form-label">Name:</label>
      <input type="text" class="form-entry" id="name" name="name" required/>
      <label for="email" class="form-label">Email:</label>
      <input type="text" class="form-entry" id="email" name="email" required/>
      <label class="form-label" for="message">Message</label>
      <textarea class="form-entry" id="message" name="message" required></textarea>
      <button type="submit">Submit</button>
    </form>
  </div>
  <script src="app.js"></script>
</body>